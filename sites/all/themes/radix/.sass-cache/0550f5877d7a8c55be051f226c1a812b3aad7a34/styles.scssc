a#to-top {
  position: fixed;
  right: 10px;
  bottom: 100px;
  width: 60px;
  height: 60px;
  background: url(../images/sprite.png) -216px 0;
  display: block;
  text-indent: -999em;
  display: none;
  &:focus {
    outline: none;
  }
}

